{
  "name": "mcs-lite-view",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "devEngines": {
    "node": "^4.2.2",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -r -f ./app/build",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js",
    "build:global": "npm run build:i18n && NODE_ENV=production DOMAIN_ENV=global npm run build",
    "build:china": "npm run build:i18n && NODE_ENV=production DOMAIN_ENV=china  npm run build",
    "build:i18n:clean": "rm -rf ./mcs-layout.pot && rm -rf ./messages",
    "build:i18n:json": "BABEL_ENV=production babel --quiet app > /dev/null",
    "build:i18n:pot": "rip json2pot './messages/**/*.json' -o './mcs-layout.pot'",
    "build:i18n:po2json": "rip po2json './*.po' -m './messages/app/**/*.json' -o './messages/translations.json'",
    "build:i18n": "npm run build:i18n:clean && npm run build:i18n:json && npm run build:i18n:pot && npm run build:i18n:po2json",
    "start": "http-server app/build/ -p 8081",
    "watch": "webpack-dev-server",
    "watch:global": "NODE_ENV=development DOMAIN_ENV=global npm run watch",
    "watch:china": " NODE_ENV=development DOMAIN_ENV=china  npm run watch",
    "lint": "eslint ./",
    "test": "NODE_ENV='test' TZ='UTC' jest --coverage --runInBand",
    "test:watch": "npm run test -- --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/components/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|styl|scss|sass)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.x",
    "babel-loader": "^6.2.10",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
