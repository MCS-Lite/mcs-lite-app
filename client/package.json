    "font-awesome-webpack": "0.0.4",
    "http-client": "^4.3.1",
    "image-webpack-loader": "^1.6.2",
    "img-loader": "^1.2.0",
    "json-loader": "^0.5.2",
    "mcs-displaycard-integer": "0.0.2",
    "mcs-lite-connect": "^0.3.7",
    "mcs-lite-icon": "^0.3.7",
    "mcs-lite-theme": "^0.3.6",
